{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "__MSG_extensionVersion__",
  "default_locale": "en",
  "icons": {
    "16": "icons/newspaper-fill-16px.png",
    "48": "icons/newspaper-fill-48px.png",
    "128": "icons/newspaper-fill-120px.png"
  },
  "permissions": ["activeTab", "scripting", "tabs", "contextMenus", "storage"],
  "host_permissions": [
    "https://www.businessdailyafrica.com/",
    "https://nation.africa/",
    "https://www.theeastafrican.co.ke/",
    "https://www.monitor.co.ug/",
    "http://www.standardmedia.co.ke/",
    "https://www.mwananchi.co.tz/",
    "https://www.newvision.co.ug/",
    "https://www.thecitizen.co.tz/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icons/newspaper-fill-48px.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
